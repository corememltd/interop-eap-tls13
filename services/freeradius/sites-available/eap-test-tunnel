server eap-test-inner {

authorize {
	filter_username

	if (User-Name == "bob") {
		update control {
			Cleartext-Password := "bob"
		}
	}

	mschap
	suffix
	update control {
		&Proxy-To-Realm := LOCAL
	}
	eap-test {
		ok = return
	}
	files
	pap
}

authenticate {
	pap
	mschap
	eap-test
}

post-auth {
	update reply {
		&Tunnel-Private-Group-ID = "test"
	}
}

}
